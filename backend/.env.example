# Copy to .env and fill values for production
NODE_ENV=production
PORT=4000
# Comma-separated list of allowed origins for CORS, e.g. https://example.com,https://admin.example.com
CORS_ORIGIN=
# Set to true if behind Nginx/Cloudflare so secure cookies work
TRUST_PROXY=true
# Strong random string
SESSION_SECRET=replace-with-strong-secret
# Optional: first-time admin default password
ADMIN_DEFAULT_PASSWORD=ChangeMe123!
